{
  "name": "infoexam/media",
  "description": "Infoexam media.",
  "type": "library",
  "homepage": "https://github.com/Infoexam/media",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-gd": "*",
    "hashids/hashids": "^2.0",
    "spatie/laravel-medialibrary": "^4.10",
    "symfony/http-foundation": "3.1.*"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.3",
    "illuminate/filesystem": "^5.3",
    "orchestra/testbench": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "suggest": {
    "ext-gmp": "Allows for arbitrary-length integers to be worked"
  },
  "autoload": {
    "psr-4": {
      "Infoexam\\Media\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "notify-on-install": false
  }
}
